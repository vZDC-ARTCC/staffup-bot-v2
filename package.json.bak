{
  "name": "vzdc-bot",
  "version": "1.0.0",
  "description": "Discord Bot",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src --ext .js,.ts",
    "dev": "tsc-watch --project . --outDir ./dist --onSuccess \"nodemon ./dist/index.js\"",
